{
    "manifest_version": 3,
    "name": "Balbum",
    "version": "1.0",
    "description": "Bookmarks Album",
    "action": {
        "default_popup": "popup.html",
        "default_title": "This is Balbum"
    },
    "options_page": "options.html",
    "permissions": [
        "bookmarks",
        "tabs",
        "storage",
        "notifications",
        "downloads"
    ],
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png" }
}